@{
    ViewData["Title"] = "Crew Page";
}

<div class="splash-title">
    <h1 class="display-4">Crew</h1>

      @foreach(Bandit bandit in Model)
      {
        <p> @bandit.Name The
        @if(bandit.Strength > 9)
        {
          <span>Mighty</span>
        }
        else
        {
          <span>Weak</span>
        }
        @bandit.ClassName
        </p>
      }
</div>
